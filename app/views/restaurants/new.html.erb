<h2>Create new restaurant</h2>

<%= form_for(@restaurant) do |f| %>
<div class="form-group">
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :address %>
<%= f.text_field :address %>
</div>
<div class="form-group">
<%= f.label :category %>
<%= f.text_field :category %>
<%= f.label :phone_number %>
<%= f.text_field :phone_number %>
</div>

<%= f.submit %>

<% end %>

